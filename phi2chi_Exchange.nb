Cell[TextData[{
 "To obtain this for ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[Phi]", "2"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "68f05852-3fd9-1f49-9a14-b20b14cc727b"],
 "\[Chi] instead, we must take n=3, and consider only the i=2 terms. To see \
why this works, consider the massless bulk-boundary propagator"
}], "Text",
 CellChangeTimes->{{3.945242182135784*^9, 3.945242198921623*^9}, {
   3.94524238483535*^9, 3.9452424519914265`*^9}, 
   3.945242647791855*^9},ExpressionUUID->"e3ce34b1-2566-e84a-b88e-\
ab6b700d341d"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"Sqrt", "[", 
     RowBox[{"2", "/", "\[Pi]"}], "]"}], 
    SuperscriptBox[
     RowBox[{"k", " "}], 
     RowBox[{"3", "/", "2"}]], 
    SuperscriptBox["z", 
     RowBox[{"3", "/", "2"}]], 
    RowBox[{"BesselK", "[", 
     RowBox[{
      RowBox[{"3", "/", "2"}], ",", 
      RowBox[{"k", " ", "z"}]}], "]"}]}], "//", "Simplify"}]}]], "Input",
 CellChangeTimes->{{3.9452424541765537`*^9, 3.9452425167798367`*^9}},
 CellLabel->"In[26]:=",ExpressionUUID->"db07d73c-2b47-7943-8d99-c567509f6c55"],

Cell[BoxData[
 RowBox[{
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{
    RowBox[{"-", "k"}], " ", "z"}]], " ", 
  RowBox[{"(", 
   RowBox[{"1", "+", 
    RowBox[{"k", " ", "z"}]}], ")"}]}]], "Output",
 CellChangeTimes->{{3.9452424960873127`*^9, 3.945242517495325*^9}},
 CellLabel->"Out[26]=",ExpressionUUID->"30249bc5-8082-a144-8067-a030f3bf3321"]
}, Open  ]],

Cell["and compare to the conformally coupled one", "Text",
 CellChangeTimes->{{3.9452426504604664`*^9, 
  3.9452426571905937`*^9}},ExpressionUUID->"443ffa82-24fa-5640-8fbd-\
98ca65e7956b"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"Sqrt", "[", 
     RowBox[{"2", "/", "\[Pi]"}], "]"}], 
    SuperscriptBox[
     RowBox[{"k", " "}], 
     RowBox[{"1", "/", "2"}]], 
    SuperscriptBox["z", 
     RowBox[{"3", "/", "2"}]], 
    RowBox[{"BesselK", "[", 
     RowBox[{
      RowBox[{"1", "/", "2"}], ",", 
      RowBox[{"k", " ", "z"}]}], "]"}]}], "//", "Simplify"}]}]], "Input",
 CellChangeTimes->{{3.9452426650846243`*^9, 3.945242688214697*^9}},
 CellLabel->"In[28]:=",ExpressionUUID->"fca9295c-13ab-2042-ab8b-9c1863e042da"],

Cell[BoxData[
 RowBox[{
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{
    RowBox[{"-", "k"}], " ", "z"}]], " ", "z"}]], "Output",
 CellChangeTimes->{{3.9452426677218285`*^9, 3.945242689230957*^9}},
 CellLabel->"Out[28]=",ExpressionUUID->"7ea48a3f-77b4-744a-9eae-c9fd6a982b14"]
}, Open  ]],

Cell[TextData[{
 "to see that if we take only the term with a power of z in the former it \
gives us the latter up to some overall factor which we were neglecting \
anyway. For vertices with two bulk-boundary propagators, this means we must \
take only the ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["z", "2"], TraditionalForm]],ExpressionUUID->
  "539a9036-1459-1c49-a33c-fc9ac21ede52"],
 " term which precisely corresponds to i=2 in the sum. Therefore at the left \
vertex of the ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Phi]", "2"], "\[Chi]"}], TraditionalForm]],
  ExpressionUUID->"2ec77823-db9b-2b4e-9e6e-e88f114b3852"],
 " exchange diagram the auxiliary propagator is "
}], "Text",
 CellChangeTimes->{{3.9452429639642344`*^9, 3.9452430671072*^9}, {
  3.946962317746107*^9, 
  3.946962323313156*^9}},ExpressionUUID->"652515ba-9cc6-8045-97fa-\
60d91d51254c"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"a0", "=", "1"}], ";", " ", 
   RowBox[{"a1", "=", "p"}], ";", " ", 
   RowBox[{"a", "=", 
    RowBox[{"{", 
     RowBox[{"a0", ",", "a1"}], "}"}]}], ";"}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"b0", "=", "1"}], ";", " ", 
  RowBox[{"b1", "=", 
   RowBox[{"k1", "+", "k2"}]}], ";", " ", 
  RowBox[{"b2", "=", " ", 
   RowBox[{"k1", " ", "k2"}]}], ";", " ", 
  RowBox[{"b", "=", 
   RowBox[{"{", 
    RowBox[{"b0", ",", "b1", ",", "b2"}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"auxProp", "=", 
  RowBox[{
   RowBox[{"Cos", "[", 
    RowBox[{
     FractionBox["\[Pi]", "2"], "\[Epsilon]"}], "]"}], 
   SuperscriptBox["s1", 
    RowBox[{"-", "\[Epsilon]"}]], " ", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{
        RowBox[{"b", "[", 
         RowBox[{"[", "3", "]"}], "]"}], 
        RowBox[{"a", "[", 
         RowBox[{"[", 
          RowBox[{"j", "+", "1"}], "]"}], "]"}], 
        SuperscriptBox["s1", 
         RowBox[{"3", "-", "2", "-", "j"}]]}], 
       RowBox[{"Gamma", "[", 
        RowBox[{"4", "-", "2", "-", "j", "-", "\[Epsilon]"}], "]"}]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"s1", "+", "b1"}], ")"}], 
         RowBox[{"Cos", "[", 
          RowBox[{
           FractionBox["\[Pi]", "2"], "j"}], "]"}]}], "+", 
        RowBox[{"p", " ", 
         RowBox[{"Sin", "[", 
          RowBox[{
           FractionBox["\[Pi]", "2"], "j"}], "]"}]}]}], ")"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "0", ",", "1"}], "}"}]}], "]"}], 
   FractionBox["1", 
    RowBox[{
     SuperscriptBox["p", "2"], "+", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"s1", "+", "k1", "+", "k2"}], ")"}], "2"]}]], 
   FractionBox["1", "p"]}]}]}], "Input",
 CellChangeTimes->{{3.945243135814148*^9, 3.945243141706051*^9}, {
  3.94524329218775*^9, 3.945243334419121*^9}, {3.945243397369299*^9, 
  3.94524346342857*^9}, {3.945243550120289*^9, 3.945243561928936*^9}, {
  3.945243686818489*^9, 3.945243693091394*^9}, {3.945243764156658*^9, 
  3.945243764598909*^9}},
 CellLabel->"In[46]:=",ExpressionUUID->"2dfbb4a8-088d-6341-b978-34cc0e9a9bf1"],

Cell[BoxData[
 FractionBox[
  RowBox[{
   SuperscriptBox["s1", 
    RowBox[{"-", "\[Epsilon]"}]], " ", 
   RowBox[{"Cos", "[", 
    FractionBox[
     RowBox[{"\[Pi]", " ", "\[Epsilon]"}], "2"], "]"}], " ", 
   RowBox[{"(", 
    RowBox[{
     FractionBox[
      RowBox[{"k1", " ", "k2", " ", 
       SuperscriptBox["p", "2"]}], 
      RowBox[{"Gamma", "[", 
       RowBox[{"1", "-", "\[Epsilon]"}], "]"}]], "+", 
     FractionBox[
      RowBox[{"k1", " ", "k2", " ", "s1", " ", 
       RowBox[{"(", 
        RowBox[{"k1", "+", "k2", "+", "s1"}], ")"}]}], 
      RowBox[{"Gamma", "[", 
       RowBox[{"2", "-", "\[Epsilon]"}], "]"}]]}], ")"}]}], 
  RowBox[{"p", " ", 
   RowBox[{"(", 
    RowBox[{
     SuperscriptBox["p", "2"], "+", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"k1", "+", "k2", "+", "s1"}], ")"}], "2"]}], 
    ")"}]}]]], "Output",
 CellChangeTimes->{3.945243464630396*^9, 3.9452435633936825`*^9, 
  3.9452436939765224`*^9, 3.945243766017845*^9},
 CellLabel->"Out[48]=",ExpressionUUID->"69716ca9-fa86-e645-8aa4-48558435ac8c"]
}, Open  ]],

Cell[TextData[{
 "where we discard overall factors. Recall that this is also integrated over \
s1. There is a similar auxiliary propagator for the right vertex which is \
easily obtained by relabelling. \nWe do not need to worry about a second \
diagram in this case, as it turns out to vanish. The reader may wish to \
verify this by taking the expression for the dotted auxiliary propagator and \
performing a similar procedure. For completeness, one should also verify that \
the unsuppressed vertices of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Phi]", "2"], "\[Chi]"}], TraditionalForm]],
  ExpressionUUID->"29dcb82b-4333-e645-96f3-97197f1f1a05"],
 " are the same as those of the massless theory ( - - -) by the shadow \
action, but here we state that it is so."
}], "Text",
 CellChangeTimes->{{3.945243587293474*^9, 3.9452435973370953`*^9}, {
  3.945243630066829*^9, 3.9452437796969776`*^9}, {3.9452438531941338`*^9, 
  3.94524392956851*^9}, {3.945245579135547*^9, 
  3.945245580871788*^9}},ExpressionUUID->"3b1f16ab-c187-9d43-9351-\
6edb0ce903f8"]
}, Open  ]],

Cell[CellGroupData[{

Cell["Integration", "Section",
 CellChangeTimes->{{3.945245589092621*^9, 
  3.9452455906285095`*^9}},ExpressionUUID->"426c6ced-7633-7b46-8844-\
2b2206289991"],

Cell["The overall integrand for this exchange diagram is", "Text",
 CellChangeTimes->{
  3.9452455939231415`*^9},ExpressionUUID->"744772b3-e8e9-f149-af3b-\
a00a3fe487fc"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{"integrand", "=", 
   RowBox[{"auxProp", "*", 
    RowBox[{"(", 
     RowBox[{"auxProp", "/.", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"k1", "->", "k3"}], ",", 
        RowBox[{"k2", "->", "k4"}], ",", 
        RowBox[{"s1", "->", "s2"}]}], "}"}]}], ")"}], 
    FractionBox["1", 
     RowBox[{
      SuperscriptBox["p", "2"], "+", 
      SuperscriptBox["k", "2"]}]]}]}]}]], "Input",
 CellChangeTimes->{{3.945243781584755*^9, 3.94524380500679*^9}},
 CellLabel->"In[49]:=",ExpressionUUID->"a3983403-e12b-6e4b-ab3e-624de46acac1"],

Cell[BoxData[
 FractionBox[
  RowBox[{
   SuperscriptBox["s1", 
    RowBox[{"-", "\[Epsilon]"}]], " ", 
   SuperscriptBox["s2", 
    RowBox[{"-", "\[Epsilon]"}]], " ", 
   SuperscriptBox[
    RowBox[{"Cos", "[", 
     FractionBox[
      RowBox[{"\[Pi]", " ", "\[Epsilon]"}], "2"], "]"}], "2"], " ", 
   RowBox[{"(", 
    RowBox[{
     FractionBox[
      RowBox[{"k1", " ", "k2", " ", 
       SuperscriptBox["p", "2"]}], 
      RowBox[{"Gamma", "[", 
       RowBox[{"1", "-", "\[Epsilon]"}], "]"}]], "+", 
     FractionBox[
      RowBox[{"k1", " ", "k2", " ", "s1", " ", 
       RowBox[{"(", 
        RowBox[{"k1", "+", "k2", "+", "s1"}], ")"}]}], 
      RowBox[{"Gamma", "[", 
       RowBox[{"2", "-", "\[Epsilon]"}], "]"}]]}], ")"}], " ", 
   RowBox[{"(", 
    RowBox[{
     FractionBox[
      RowBox[{"k3", " ", "k4", " ", 
       SuperscriptBox["p", "2"]}], 
      RowBox[{"Gamma", "[", 
       RowBox[{"1", "-", "\[Epsilon]"}], "]"}]], "+", 
     FractionBox[
      RowBox[{"k3", " ", "k4", " ", "s2", " ", 
       RowBox[{"(", 
        RowBox[{"k3", "+", "k4", "+", "s2"}], ")"}]}], 
      RowBox[{"Gamma", "[", 
       RowBox[{"2", "-", "\[Epsilon]"}], "]"}]]}], ")"}]}], 
  RowBox[{
   SuperscriptBox["p", "2"], " ", 
   RowBox[{"(", 
    RowBox[{
     SuperscriptBox["k", "2"], "+", 
     SuperscriptBox["p", "2"]}], ")"}], " ", 
   RowBox[{"(", 
    RowBox[{
     SuperscriptBox["p", "2"], "+", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"k1", "+", "k2", "+", "s1"}], ")"}], "2"]}], ")"}], " ", 
   RowBox[{"(", 
    RowBox[{
     SuperscriptBox["p", "2"], "+", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"k3", "+", "k4", "+", "s2"}], ")"}], "2"]}], 
    ")"}]}]]], "Output",
 CellChangeTimes->{3.945243806692955*^9},
 CellLabel->"Out[49]=",ExpressionUUID->"3c3b0032-3d99-6c4f-a4ae-6f73d55b06cf"]
}, Open  ]],

Cell["\<\
We have found this diagram, and the massless equivalent, are far easier to \
compute if one performs the following simplification\
\>", "Text",
 CellChangeTimes->{{3.945243939672165*^9, 
  3.945243992601183*^9}},ExpressionUUID->"cf3a952c-4829-724e-940e-\
b3300425b638"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{"Apart", "[", 
   FractionBox["1", 
    RowBox[{
     SuperscriptBox["p", "2"], 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["p", "2"], "+", 
       SuperscriptBox["k", "2"]}], ")"}]}]], "]"}]}]], "Input",
 CellChangeTimes->{{3.9452439949161625`*^9, 3.945244004200573*^9}},
 CellLabel->"In[50]:=",ExpressionUUID->"a8cc2974-1304-004c-bf39-ea0e2e08aed5"],

Cell[BoxData[
 RowBox[{
  FractionBox["1", 
   RowBox[{
    SuperscriptBox["k", "2"], " ", 
    SuperscriptBox["p", "2"]}]], "-", 
  FractionBox["1", 
   RowBox[{
    SuperscriptBox["k", "2"], " ", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["k", "2"], "+", 
      SuperscriptBox["p", "2"]}], ")"}]}]]}]], "Output",
 CellChangeTimes->{3.9452440048316975`*^9},
 CellLabel->"Out[50]=",ExpressionUUID->"3b092ac4-01b7-c149-8ab1-5abca0197b2d"]
}, Open  ]],

Cell[TextData[{
 "and note that this can be written ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", 
     SuperscriptBox["k", 
      RowBox[{"2", " "}]]], 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["\[Limit]", 
       RowBox[{"k", " ", "->", "0"}]], 
      RowBox[{"-", "1"}]}], ")"}], 
    FractionBox["1", 
     RowBox[{
      SuperscriptBox["p", "2"], "+", 
      SuperscriptBox["k", "2"]}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "045665c5-fe5c-c349-b028-7d98a9ab7756"],
 " where the limit should only retain the finite piece, discarding any \
divergences (which could only have come from the residue at p=0 however this \
pole is avoided in the propagator in order to satisfy EoM). Hence we simply \
get rid of the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "/", 
    SuperscriptBox["p", 
     RowBox[{"2", " "}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "2463d31b-9d28-9147-85c9-be08b9839ee6"],
 " piece at this stage and reintroduce it by taking the above limit after \
integrating. "
}], "Text",
 CellChangeTimes->{{3.94524401582156*^9, 3.945244029342657*^9}, {
  3.9452440791022854`*^9, 
  3.9452442140304794`*^9}},ExpressionUUID->"365459cf-9dcd-a64d-a656-\
db399831ee21"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{"sIntegrand", "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"2", "I"}], 
       RowBox[{"k1", " ", "k2", " ", "k3", " ", "k4"}]], " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"Residue", "[", 
         RowBox[{
          RowBox[{
           SuperscriptBox["p", 
            RowBox[{"2", " "}]], "integrand"}], ",", 
          RowBox[{"{", 
           RowBox[{"p", ",", 
            RowBox[{"I", " ", "k"}]}], "}"}]}], "]"}], "+", 
        RowBox[{"Residue", "[", 
         RowBox[{
          RowBox[{
           SuperscriptBox["p", 
            RowBox[{"2", " "}]], "integrand"}], ",", 
          RowBox[{"{", 
           RowBox[{"p", ",", 
            RowBox[{"I", " ", 
             RowBox[{"(", 
              RowBox[{"k1", "+", "k2", "+", "s1"}], ")"}]}]}], "}"}]}], "]"}],
         "+", 
        RowBox[{"Residue", "[", 
         RowBox[{
          RowBox[{
           SuperscriptBox["p", 
            RowBox[{"2", " "}]], "integrand"}], ",", 
          RowBox[{"{", 
           RowBox[{"p", ",", 
            RowBox[{"I", " ", 
             RowBox[{"(", 
              RowBox[{"k3", "+", "k4", "+", "s2"}], ")"}]}]}], "}"}]}], 
         "]"}]}], ")"}]}], "//", "ExpandAll"}], "//", 
    "Simplify"}]}]}]], "Input",
 CellChangeTimes->{{3.945244224172035*^9, 3.9452443605095387`*^9}, {
   3.9452443999561787`*^9, 3.945244402844406*^9}, {3.9452444396198635`*^9, 
   3.9452444642813416`*^9}, 3.9452445362310524`*^9},
 CellLabel->"In[60]:=",ExpressionUUID->"3a9a5e02-5900-cc4c-b187-1ee1936396a4"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(", 
   RowBox[{
    SuperscriptBox["s1", 
     RowBox[{"-", "\[Epsilon]"}]], " ", 
    SuperscriptBox["s2", 
     RowBox[{"-", "\[Epsilon]"}]], " ", 
    SuperscriptBox[
     RowBox[{"Cos", "[", 
      FractionBox[
       RowBox[{"\[Pi]", " ", "\[Epsilon]"}], "2"], "]"}], "2"], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"s1", " ", "s2", " ", 
       RowBox[{"(", 
        RowBox[{
        "k", "+", "k1", "+", "k2", "+", "k3", "+", "k4", "+", "s1", "+", 
         "s2"}], ")"}], " ", 
       SuperscriptBox[
        RowBox[{"Gamma", "[", 
         RowBox[{"1", "-", "\[Epsilon]"}], "]"}], "2"]}], "+", 
      RowBox[{"k", " ", 
       RowBox[{"(", 
        RowBox[{
